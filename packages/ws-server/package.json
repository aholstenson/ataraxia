    "prebuild": "rimraf dist",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "ataraxia": "^0.9.1",
    "ataraxia-ws-client": "^0.9.1",
    "debug": "^4.3.1",
    "ws": "^7.4.6"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.6.1",
    "@types/ws": "^7.4.4",
    "jest": "^27.0.3",
    "ts-jest": "^27.0.1",
    "typescript": "^4.3.2"
  }
}
