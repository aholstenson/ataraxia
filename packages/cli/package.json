{
  "name": "ataraxia-cli",
  "version": "0.12.2",
  "description": "Command-line client for inspecting Ataraxia networks",
  "license": "MIT",
  "repository": "https://github.com/aholstenson/ataraxia/tree/master/packages/cli",
  "keywords": [
    "p2p",
    "mesh",
    "mesh networking",
    "messaging"
  ],
  "type": "module",
  "bin": {
    "ataraxia": "./dist/esm/index.js"
