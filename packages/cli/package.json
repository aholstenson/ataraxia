    "prebuild": "rimraf dist",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "ataraxia": "^0.12.2",
    "ataraxia-hyperswarm": "^0.12.0",
    "ataraxia-local": "^0.12.0",
    "ataraxia-tcp": "^0.12.0",
    "chalk": "^5.0.1",
    "minimatch": "^5.1.0",
    "prettyjson": "^1.2.5",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@types/minimatch": "^3.0.5",
    "@types/prettyjson": "^0.0.30"
  }
}
