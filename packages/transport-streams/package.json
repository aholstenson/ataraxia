    "build": "tsc --project tsconfig.build.json --outDir dist/esm",
    "prebuild": "rimraf dist",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@hyperswarm/secret-stream": "^5.1.4",
    "@stablelib/cbor": "^1.0.1",
    "ataraxia-transport": "^0.12.0",
    "debug": "^4.3.3"
  }
}
